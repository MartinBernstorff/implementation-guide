<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="ehealth-episodeofcare" />
  <url value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-episodeofcare" />
  <name value="ehealth-episodeofcare" />
  <title value="eHealth EpisodeOfCare" />
  <status value="draft" />
  <publisher value="ehealth.sundhed.dk" />
  <fhirVersion value="3.0.1" />
  <kind value="resource" />
  <abstract value="false" />
  <type value="EpisodeOfCare" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/EpisodeOfCare" />
  <derivation value="constraint" />
  <differential>
    <element id="EpisodeOfCare">
      <path value="EpisodeOfCare" />
      <short value="An association between a patient and an organization / healthcare provider(s) during which time encounters may occur." />
    </element>
    <element id="EpisodeOfCare.extension">
      <path value="EpisodeOfCare.extension" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="url" />
        </discriminator>
        <rules value="open" />
      </slicing>
    </element>
    <element id="EpisodeOfCare.extension:careManager">
      <path value="EpisodeOfCare.extension" />
      <sliceName value="careManager" />
      <short value="Care managers/care coordinators." />
      <definition value="Care managers/care coordinators for the patient." />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-episodeofcare-careManagers" />
      </type>
    </element>
    <element id="EpisodeOfCare.extension:careManagerHistory">
      <path value="EpisodeOfCare.extension" />
      <sliceName value="careManagerHistory" />
      <short value="History of care managers/care coordinators for the patient." />
      <definition value="History of care managers/care coordinators for the patient. When a new care manager is assigned the previous one is moved to here and assigned a period." />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-episodeofcare-careManagerHistory" />
      </type>
    </element>
    <element id="EpisodeOfCare.extension:responsible">
      <path value="EpisodeOfCare.extension" />
      <sliceName value="responsible" />
      <short value="Individuals, careteam or organization responsible for monitoring this plan." />
      <definition value="Individuals, careteam and/or organization who was responsible for monitoring this plan in the specified period." />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-responsible" />
      </type>
    </element>
    <element id="EpisodeOfCare.extension:responsibleHistory">
      <path value="EpisodeOfCare.extension" />
      <sliceName value="responsibleHistory" />
      <short value="Previous individuals, careteams and organizations responsible for monitoring this plan." />
      <definition value="Previous individuals, careteams and organizations responsible for monitoring this plan in the specified period." />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-responsibleHistory" />
      </type>
    </element>
    <element id="EpisodeOfCare.diagnosis">
      <extension url="http://hl7.org/fhir/StructureDefinition/structuredefinition-explicit-type-name">
        <valueString value="Diagnosis" />
      </extension>
      <path value="EpisodeOfCare.diagnosis" />
      <min value="1" />
    </element>
    <element id="EpisodeOfCare.patient">
      <path value="EpisodeOfCare.patient" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-patient" />
      </type>
    </element>
    <element id="EpisodeOfCare.managingOrganization">
      <path value="EpisodeOfCare.managingOrganization" />
      <min value="1" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-organization" />
      </type>
    </element>
    <element id="EpisodeOfCare.period">
      <path value="EpisodeOfCare.period" />
      <min value="1" />
    </element>
    <element id="EpisodeOfCare.careManager">
      <path value="EpisodeOfCare.careManager" />
      <max value="0" />
    </element>
    <element id="EpisodeOfCare.team">
      <path value="EpisodeOfCare.team" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-careteam" />
      </type>
    </element>
    <element id="EpisodeOfCare.account">
      <path value="EpisodeOfCare.account" />
      <max value="0" />
    </element>
  </differential>
</StructureDefinition>