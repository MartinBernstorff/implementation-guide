        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-organization-relatedTo" />
      </type>
    </element>
    <element id="Organization.extension:cvrNumber">
      <path value="Organization.extension" />
      <sliceName value="cvrNumber" />
