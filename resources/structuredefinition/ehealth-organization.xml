      <max value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-organization-telecomSystem" />
      </type>
    </element>
    <element id="Organization.contact.telecom.extension:telecomValue">
      <path value="Organization.contact.telecom.extension" />
      <sliceName value="telecomValue" />
      <short value="The actual contact point details (i.e. phone number or email address)" />
