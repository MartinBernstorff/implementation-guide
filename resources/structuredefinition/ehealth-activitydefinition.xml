<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="ehealth-activitydefinition" />
  <url value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-activitydefinition" />
  <name value="ehealth-activitydefinition" />
  <title value="eHealth ActivityDefinition" />
  <status value="draft" />
  <publisher value="ehealth.sundhed.dk" />
  <fhirVersion value="3.0.1" />
  <kind value="resource" />
  <abstract value="false" />
  <type value="ActivityDefinition" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/ActivityDefinition" />
  <derivation value="constraint" />
  <differential>
    <element id="ActivityDefinition">
      <path value="ActivityDefinition" />
      <short value="The definition of a specific activity to be taken, independent of any particular patient or context." />
    </element>
    <element id="ActivityDefinition.extension">
      <path value="ActivityDefinition.extension" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="url" />
        </discriminator>
        <rules value="open" />
      </slicing>
    </element>
    <element id="ActivityDefinition.extension:recommendation">
      <path value="ActivityDefinition.extension" />
      <sliceName value="recommendation" />
      <short value="Recommendation state of the plan definition." />
      <definition value="Defines the recommendation state of the plan definition." />
      <max value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-recommendation" />
      </type>
      <binding>
        <strength value="required" />
        <description value="Value set of plan definition recommendation codes." />
        <valueSetUri value="http://ehealth.sundhed.dk/vs/plandefinition-recommendation" />
      </binding>
    </element>
    <element id="ActivityDefinition.extension:intendedAudience">
      <path value="ActivityDefinition.extension" />
      <sliceName value="intendedAudience" />
      <short value="Intended audience for the plan." />
      <definition value="Specifies that the plan is only intended for use in the specified organizations. Leave blank if the plan is intended for global use." />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-intendedAudience" />
      </type>
    </element>
    <element id="ActivityDefinition.extension:referenceRange">
      <path value="ActivityDefinition.extension" />
      <sliceName value="referenceRange" />
      <short value="Provides guide for interpretation." />
      <definition value="Provides guide for interpretation&#xD;&#xA;+ Must have at least a low or a high or text" />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-referenceRange" />
      </type>
    </element>
    <element id="ActivityDefinition.extension:modifierRole">
      <path value="ActivityDefinition.extension" />
      <sliceName value="modifierRole" />
      <short value="Stakeholders involved in maintenance of this ActivityDefinition." />
      <definition value="Stakeholders involved in maintenance of this ActivityDefinition." />
      <min value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-modifier-role" />
      </type>
    </element>
    <element id="ActivityDefinition.extension:modifierRole.extension:reference">
      <path value="ActivityDefinition.extension.extension" />
      <sliceName value="reference" />
      <short value="Stakeholder playing a role in maintaining the ActivityDefinition." />
      <definition value="Stakeholder playing a role in maintaining the ActivityDefinition." />
    </element>
    <element id="ActivityDefinition.extension:modifierRole.extension:role">
      <path value="ActivityDefinition.extension.extension" />
      <sliceName value="role" />
      <short value="Role this stakeholder plays in maintaining the ActivityDefinition. Only some roles are allowed to update." />
      <definition value="The role played by the &quot;reference&quot; resource in modifying and maintaining the ActivityDefinition. Only some roles are allowed to update." />
    </element>
    <element id="ActivityDefinition.extension:minimumQuality">
      <path value="ActivityDefinition.extension" />
      <sliceName value="minimumQuality" />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-quality" />
      </type>
    </element>
    <element id="ActivityDefinition.version">
      <path value="ActivityDefinition.version" />
      <min value="1" />
    </element>
    <element id="ActivityDefinition.topic">
      <path value="ActivityDefinition.topic" />
      <min value="1" />
      <max value="1" />
      <binding>
        <extension url="http://hl7.org/fhir/StructureDefinition/elementdefinition-bindingName">
          <valueString value="DefinitionTopic" />
        </extension>
        <strength value="required" />
        <description value="Treatment | Self-treatment" />
        <valueSetReference>
          <reference value="http://ehealth.sundhed.dk/vs/topic-type" />
        </valueSetReference>
      </binding>
    </element>
    <element id="ActivityDefinition.relatedArtifact">
      <path value="ActivityDefinition.relatedArtifact" />
      <type>
        <code value="RelatedArtifact" />
        <profile value="http://hl7.org/fhir/StructureDefinition/RelatedArtifact" />
      </type>
    </element>
    <element id="ActivityDefinition.library">
      <path value="ActivityDefinition.library" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-library" />
        <aggregation value="referenced" />
      </type>
    </element>
    <element id="ActivityDefinition.code">
      <path value="ActivityDefinition.code" />
      <min value="1" />
      <binding>
        <extension url="http://hl7.org/fhir/StructureDefinition/elementdefinition-bindingName">
          <valueString value="ActivityDefinitionType" />
        </extension>
        <strength value="required" />
        <valueSetUri value="http://ehealth.sundhed.dk/vs/activitydefinition-code" />
      </binding>
    </element>
  </differential>
</StructureDefinition>