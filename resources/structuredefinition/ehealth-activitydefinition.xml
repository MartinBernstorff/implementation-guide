      <definition value="Maintainers of the activity definition. Identifies practitioners, care teams and organizations and their responsibility as maintainers." />
      <min value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-modifier-role" />
      </type>
    </element>
    <element id="ActivityDefinition.extension:minimumQuality">
      <path value="ActivityDefinition.extension" />
      <sliceName value="minimumQuality" />
      <short value="Minimum requirements for qualities." />
      <definition value="Minimum requirements for qualities" />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-quality" />
      </type>
    </element>
    <element id="ActivityDefinition.extension:sharingPolicy">
      <path value="ActivityDefinition.extension" />
      <sliceName value="sharingPolicy" />
      <min value="1" />
      <max value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-sharingPolicy" />
      </type>
    </element>
    <element id="ActivityDefinition.version">
      <path value="ActivityDefinition.version" />
      <min value="1" />
    </element>
    <element id="ActivityDefinition.topic">
      <path value="ActivityDefinition.topic" />
      <min value="1" />
      <max value="1" />
      <binding>
        <extension url="http://hl7.org/fhir/StructureDefinition/elementdefinition-bindingName">
          <valueString value="DefinitionTopic" />
        </extension>
        <strength value="required" />
        <description value="Treatment | Self-treatment" />
        <valueSetReference>
          <reference value="http://ehealth.sundhed.dk/vs/topic-type" />
        </valueSetReference>
      </binding>
    </element>
    <element id="ActivityDefinition.relatedArtifact">
      <path value="ActivityDefinition.relatedArtifact" />
      <type>
        <code value="RelatedArtifact" />
        <profile value="http://hl7.org/fhir/StructureDefinition/RelatedArtifact" />
      </type>
    </element>
    <element id="ActivityDefinition.library">
      <path value="ActivityDefinition.library" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-library" />
        <aggregation value="referenced" />
      </type>
    </element>
    <element id="ActivityDefinition.code">
      <path value="ActivityDefinition.code" />
      <min value="1" />
      <binding>
        <extension url="http://hl7.org/fhir/StructureDefinition/elementdefinition-bindingName">
          <valueString value="ActivityDefinitionType" />
        </extension>
        <strength value="required" />
        <valueSetUri value="http://ehealth.sundhed.dk/vs/activitydefinition-code" />
      </binding>
    </element>
    <element id="ActivityDefinition.location">
      <path value="ActivityDefinition.location" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://hl7.org/fhir/StructureDefinition/Location" />
        <aggregation value="referenced" />
      </type>
    </element>
    <element id="ActivityDefinition.product[x]">
      <path value="ActivityDefinition.product[x]" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://hl7.org/fhir/StructureDefinition/Medication" />
        <aggregation value="referenced" />
      </type>
      <type>
        <code value="Reference" />
        <targetProfile value="http://hl7.org/fhir/StructureDefinition/Substance" />
        <aggregation value="referenced" />
      </type>
      <type>
        <code value="CodeableConcept" />
      </type>
    </element>
    <element id="ActivityDefinition.transform">
      <path value="ActivityDefinition.transform" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://hl7.org/fhir/StructureDefinition/StructureMap" />
        <aggregation value="referenced" />
      </type>
    </element>
  </differential>
