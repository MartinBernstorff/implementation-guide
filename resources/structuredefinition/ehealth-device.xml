<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="ehealth-device" />
  <url value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-device" />
  <name value="ehealth-device" />
  <title value="eHealth Device" />
  <status value="draft" />
  <publisher value="ehealth.sundhed.dk" />
  <fhirVersion value="3.0.1" />
  <kind value="resource" />
  <abstract value="false" />
  <type value="Device" />
  <baseDefinition value="http://hl7.org/fhir/StructureDefinition/Device" />
  <derivation value="constraint" />
  <differential>
    <element id="Device">
      <path value="Device" />
      <short value="An instance or a type of a manufactured item that is used in the provision of healthcare." />
    </element>
    <element id="Device.extension">
      <path value="Device.extension" />
      <slicing>
        <discriminator>
          <type value="value" />
          <path value="url" />
        </discriminator>
        <rules value="open" />
      </slicing>
    </element>
    <element id="Device.extension:privatelyOwned">
      <path value="Device.extension" />
      <sliceName value="privatelyOwned" />
      <short value="True if this device is owned or provided by the patient." />
      <definition value="True if this device is owned or provided by the patient." />
      <max value="1" />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-device-privatelyOwned" />
      </type>
    </element>
    <element id="Device.extension:suppliers">
      <path value="Device.extension" />
      <sliceName value="suppliers" />
      <short value="Different types of suppliers for this device." />
      <definition value="Different types of suppliers for this device. E.g. provisioners, maintainers, calibrators, decommissioners, trainers." />
      <type>
        <code value="Extension" />
        <profile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-device-suppliers" />
      </type>
    </element>
    <element id="Device.type">
      <path value="Device.type" />
      <binding>
        <extension url="http://hl7.org/fhir/StructureDefinition/elementdefinition-bindingName">
          <valueString value="DeviceKind" />
        </extension>
        <strength value="required" />
        <valueSetReference>
          <reference value="http://ehealth.sundhed.dk/vs/device-types" />
        </valueSetReference>
      </binding>
    </element>
    <element id="Device.patient">
      <path value="Device.patient" />
      <max value="0" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-patient" />
      </type>
    </element>
    <element id="Device.owner">
      <path value="Device.owner" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-organization" />
      </type>
    </element>
    <element id="Device.note.author[x]">
      <path value="Device.note.author[x]" />
      <type>
        <code value="Reference" />
        <targetProfile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-practitioner" />
        <aggregation value="referenced" />
      </type>
      <type>
        <code value="Reference" />
        <targetProfile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-patient" />
        <aggregation value="referenced" />
      </type>
      <type>
        <code value="Reference" />
        <targetProfile value="http://ehealth.sundhed.dk/fhir/StructureDefinition/ehealth-relatedperson" />
        <aggregation value="referenced" />
      </type>
      <type>
        <code value="string" />
      </type>
    </element>
  </differential>
</StructureDefinition>