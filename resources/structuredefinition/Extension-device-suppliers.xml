      <binding>
        <strength value="required" />
        <valueSetUri value="http://ehealth.sundhed.dk/vs/device-supplier-roles" />
      </binding>
    </element>
    <element id="Extension.extension:role.url">
      <path value="Extension.extension.url" />
      <fixedUri value="role" />
    </element>
    <element id="Extension.extension:role.value[x]:valueCodeableConcept">
      <path value="Extension.extension.valueCodeableConcept" />
      <sliceName value="valueCodeableConcept" />
      <type>
        <code value="CodeableConcept" />
      </type>
    </element>
    <element id="Extension.url">
      <path value="Extension.url" />
      <fixedUri value="http://example.org/fhir/StructureDefinition/device-suppliers" />
    </element>
    <element id="Extension.value[x]">
      <path value="Extension.value[x]" />
      <max value="0" />
    </element>
  </differential>
