<?xml version="1.0" encoding="utf-8"?>
<StructureDefinition xmlns="http://hl7.org/fhir">
  <id value="device-suppliers" />
  <url value="http://example.org/fhir/StructureDefinition/device-suppliers" />
  <name value="suppliers" />
