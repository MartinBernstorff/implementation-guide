<OperationDefinition xmlns="http://hl7.org/fhir">
    <id value="-s-fetch-careteam-stats"/>
    <name value="fetch-careteam-stats"/>
    <status value="active"/>
    <kind value="operation"/>
    <description value="Return a report with care teams, summarizing counts of care plans, episodes of care, and tasks by category, grouped by care team"/>
    <idempotent value="false"/>
    <code value="fetch-careteam-stats"/>
    <system value="true"/>
    <type value="false"/>
    <instance value="false"/>
    <parameter>
        <name value="anonymization"/>
        <use value="in"/>
        <min value="1"/>
        <max value="1"/>
        <type value="string"/>
        <documentation value="Use anonymization mode [NONE, ANONYMIZED]. " />
    </parameter>
    <parameter>
        <name value="organization"/>
        <use value="in"/>
        <min value="0"/>
        <max value="1"/>
        <type value="Reference"/>
        <documentation value="Only care teams for organization. " />
    </parameter>
    <parameter>
        <name value="period"/>
        <use value="in"/>
        <min value="0"/>
        <max value="1"/>
        <type value="Period"/>
        <documentation value="Only episodes of care, care plans, and tasks within period. " />
    </parameter>
</OperationDefinition>