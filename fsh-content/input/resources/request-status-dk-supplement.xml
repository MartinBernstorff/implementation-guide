<CodeSystem xmlns="http://hl7.org/fhir">

   
   <supplements value="http://hl7.org/fhir/request-status" ></supplements>


   <id value="request-status-dk-supplement"></id>
  
   <url value="http://hl7.org/fhir/request-status"></url>
   <version value="4.0.1"></version>
   <name value="RequestStatus"></name>
   <status value="draft"></status>
   <experimental value="false"></experimental>
   <publisher value="HL7 DK"></publisher>
   <contact>
      <telecom>
         <system value="url"></system>
         <value value="http://hl7.dk"></value>
      </telecom>
   </contact>
   <description value="Codes identifying the stage lifecycle stage of a request"></description>
   <caseSensitive value="true"></caseSensitive>
   
   <content value="complete"></content>
   <concept>
      <code value="draft"></code>
      <display value="Draft"></display>
      <definition value="The request has been created but is not yet complete or ready for action."></definition>
      <designation>
         <language value="da"></language>
         <use>
            <system value="http://snomed.info/sct"></system>
            <code value="900000000000013009"></code>
         </use>
         <value value="Udkast"></value>
      </designation>
   </concept>
   <concept>
      <code value="active"></code>
      <display value="Active"></display>
      <definition value="The request is in force and ready to be acted upon."></definition>
      <designation>
         <language value="da"></language>
         <use>
            <system value="http://snomed.info/sct"></system>
            <code value="900000000000013009"></code>
         </use>
         <value value="Aktiv"></value>
      </designation>
   </concept>
   <concept>
      <code value="on-hold"></code>
      <display value="On Hold"></display>
      <definition value="The request (and any implicit authorization to act) has been temporarily withdrawn but is expected to resume in the future."></definition>
      <designation>
         <language value="da"></language>
         <use>
            <system value="http://snomed.info/sct"></system>
            <code value="900000000000013009"></code>
         </use>
         <value value="Suspenderet"></value>
      </designation>
   </concept>
   <concept>
      <code value="revoked"></code>
      <display value="Revoked"></display>
      <definition value="The request (and any implicit authorization to act) has been terminated prior to the known full completion of the intended actions. No further activity should occur."></definition>
      <designation>
         <language value="da"></language>
         <use>
            <system value="http://snomed.info/sct"></system>
            <code value="900000000000013009"></code>
         </use>
         <value value="Annulleret"></value>
      </designation>
   </concept>
   <concept>
      <code value="completed"></code>
      <display value="Completed"></display>
      <definition value="The activity described by the request has been fully performed. No further activity will occur."></definition>
      <designation>
         <language value="da"></language>
         <use>
            <system value="http://snomed.info/sct"></system>
            <code value="900000000000013009"></code>
         </use>
         <value value="Afsluttet"></value>
      </designation>
   </concept>
   <concept>
      <code value="entered-in-error"></code>
      <display value="Entered in Error"></display>
      <definition value="This request should never have existed and should be considered 'void'. (It is possible that real-world decisions were based on it. If real-world activity has occurred, the status should be &quot;revoked&quot; rather than &quot;entered-in-error&quot;.)."></definition>
      <designation>
         <language value="da"></language>
         <use>
            <system value="http://snomed.info/sct"></system>
            <code value="900000000000013009"></code>
         </use>
         <value value="Fejlindtastning"></value>
      </designation>
   </concept>
   <concept>
      <code value="unknown"></code>
      <display value="Unknown"></display>
      <definition value="The authoring system does not know which of the status values currently applies for this request.  Note: This concept is not to be used for &quot;other&quot;  - one of the listed statuses is presumed to apply, but the authoring/source system does not know which."></definition>
      <designation>
         <language value="da"></language>
         <use>
            <system value="http://snomed.info/sct"></system>
            <code value="900000000000013009"></code>
         </use>
         <value value="Ukendt"></value>
      </designation>
   </concept>
</CodeSystem>
